{
  "name": "Converter VK",
  "description": "С латиницы на кириллицу",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://vk.com/*"],
      "js": ["js/main.js"],
      "run_at": "document_end"
    }
  ]

}